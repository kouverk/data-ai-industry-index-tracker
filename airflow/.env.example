# Astronomer/Airflow Environment Variables
# Copy to .env and fill in values

# Snowflake Connection
SNOWFLAKE_ACCOUNT=your_account
SNOWFLAKE_USER=your_user
SNOWFLAKE_PASSWORD=your_password
SNOWFLAKE_DATABASE=KOUVERK_DATA_INDUSTRY
SNOWFLAKE_WAREHOUSE=your_warehouse
SNOWFLAKE_SCHEMA=raw

# GitHub API (optional, increases rate limit from 60 to 5000 requests/hour)
GITHUB_TOKEN=ghp_your_token_here

# Anthropic API (for weekly insights)
ANTHROPIC_API_KEY=sk-ant-your_key_here

# Airflow (for local docker-compose only)
AIRFLOW_UID=50000
